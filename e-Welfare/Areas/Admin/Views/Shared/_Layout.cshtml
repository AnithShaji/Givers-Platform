<script>


    function LoadTab(tab) {

        $(".overlay").show();
        $("#content").load('@Url.Action("LoadContent", "ClientListing", new { Area = "Client" })', { tab: tab, userType:"ADMN" }, function () {


                setTimeout(function () { $(".overlay").hide(); }, 1000);
            });

    }

        function LoadPermissionTab(tab) {
       $(".overlay").show();
            $("#content").load('@Url.Action("LoadPermission", "AdminDashboard", new { Area = "Admin" })',  function () {


                setTimeout(function () { $(".overlay").hide(); }, 1000);
            });
        }
</script>
