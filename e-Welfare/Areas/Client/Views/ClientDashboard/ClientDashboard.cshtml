
<script type="text/javascript">
    // In the view:
      $(document).ready(function () {
        @if (TempData["SucessAlert"] != null && TempData["SucessAlert"].ToString()=="1")
        {
            @:SuccesMessage("Saved successfully.", "Information");
            TempData.Remove("SucessAlert");
        }
        else if (TempData["SucessAlert"] != null && TempData["SucessAlert"].ToString()=="0")
        {
            @:ErrorMessage("Error while saving the records.", "Warning");
            TempData.Remove("SucessAlert");
        }
        else if (TempData["SucessAlert"] != null && TempData["SucessAlert"].ToString()=="2")
        {
            @:SuccesMessage("Updated successfully", "Information");
            TempData.Remove("SucessAlert");
        }
    });
    </script>


<div id="content" class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Dashboard <small>Client</small>
            </h1>
            <ol class="breadcrumb">
                <li class="active">
                    <i class="fa fa-dashboard"></i> Dashboard
                </li>
            </ol>
        </div>
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="alert alert-success alert-dismissable">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                <i class="fa fa-info-circle"></i>
                @if (TempData["UserStatus"] != null && TempData["UserStatus"].ToString() == "INP")
                {

                    <strong>Application In Process</strong>
                }
                else if (TempData["UserStatus"] != null && TempData["UserStatus"].ToString() == "ACC")
                {
                    <strong>Application Is Accepted</strong>
                }
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div id="contentTbl">
                @*@if (TempData["UserStatus"] != null && TempData["UserStatus"].ToString() == "INP")
                    {*@
                @{ Html.RenderAction("ClientDashboardType", "ClientDashboard", new { clientDashboardType = TempData["UserStatus"].ToString() }); }
                @* } *@
            </div>
        </div>
    </div>

    <!-- /.row -->
</div>


<script>

    $(document).ready(function () {
        // On first load

                 if ("@TempData["UserStatus"].ToString()" == "INP") {
                $('#FoodTab').remove();
                $('#MedTab').remove();
                $('#EduTab').remove();
            }
            else
        if ("@TempData["WelfareSection"]" != "" ) {

            if ("@TempData["WelfareSection"]" == "E"){
                $('#FoodTab').remove();
                $('#MedTab').remove();
            } else if ("@TempData["WelfareSection"]" == "M") {
                $('#FoodTab').remove();
                $('#EduTab').remove();
            } else {
                $('#EduTab').remove();
                $('#MedTab').remove();
            }

        };
    });
</script>
















